{
  "name": "lienflash-fe",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hot-loader": "^4.3.11",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "build": "webpack --mode development",
    "test": "jest",
    "start": "webpack-dev-server --mode development"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "modulePaths": [
      "/shared/vendor/modules"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/reactTests/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/client/src/stylesheets/",
      "^react(.*)$": "<rootDir>/vendor/react-master$1",
      "^config$": "<rootDir>/configs/app-config.js"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/legacy_notes_and_workbook/",
      "<rootDir>/client/src/stylesheets/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lienflash/lienflash-fe.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lienflash/lienflash-fe/issues"
  },
  "homepage": "https://github.com/lienflash/lienflash-fe#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-arrow-functions": "^7.12.1",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/plugin-transform-template-literals": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^26.6.1",
    "babel-loader": "^8.0.2",
    "css-loader": "^1.0.0",
    "eslint": "^7.11.0",
    "eslint-webpack-plugin": "^2.1.0",
    "file-loader": "^6.1.1",
    "jest": "^22.4.2",
    "react-test-renderer": "^16.0.0-alpha.12",
    "sass": "^1.27.0",
    "sass-loader": "^10.0.4",
    "style-loader": "^0.23.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  }
}
